{
  "name": "reserve-rec-api",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "echo \"The build step is not required when using JavaScript!\" && exit 0",
    "cdk": "cdk",
    "test": "jest",
    "synth:local": "cdk synth -c @context=local --all",
    "synth:dev": "cdk synth -c @context=dev --all",
    "deploy:dev": "cdk deploy -c @context=dev --all",
    "run-admin": "sam local start-api -t cdk.out/sam-AdminApiStack-Local-AdminApi.yaml --env-vars env.json --warm-containers LAZY --skip-pull-image 2>&1 | tr '\r' '\n'",
    "run-admin-full": "yarn synth:local && yarn run-admin"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.828.0",
    "@aws-sdk/client-cloudformation": "^3.828.0",
    "@aws-sdk/client-dynamodb": "^3.879.0",
    "@aws-sdk/client-lambda": "^3.828.0",
    "@aws-sdk/client-s3": "^3.906.0",
    "@aws-sdk/client-ses": "^3.906.0",
    "@aws-sdk/client-sqs": "^3.906.0",
    "@aws-sdk/client-ssm": "^3.828.0",
    "@aws-sdk/credential-provider-node": "^3.743.0",
    "@aws-sdk/lib-dynamodb": "^3.879.0",
    "@aws-sdk/util-dynamodb": "^3.828.0",
    "@opensearch-project/opensearch": "^3.2.0",
    "aws-cdk-lib": "2.173.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "constructs": "^10.0.0",
    "crypto": "^1.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.6.1",
    "node-rsa": "^1.0.2",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  }
}
