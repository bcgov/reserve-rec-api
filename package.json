{
  "name": "reserve-rec-api",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "echo \"The build step is not required when using JavaScript!\" && exit 0",
    "cdk": "cdk",
    "test": "jest --coverage",
    "test:nocov": "jest",
    "synth:local": "cdk synth -c @context=local --all",
    "build:admin": "node ./src/scripts/tools/local-testing/sam-quick-start.js",
    "build:layers": "node ./src/scripts/tools/local-testing/build-lambda-layers.js",
    "build:public": "node ./src/scripts/tools/local-testing/generate-sam-templates.js",
    "synth:dev": "cdk synth -c @context=dev -q --all",
    "synth:test": "cdk synth -c @context=test -q --all",
    "deploy:dev": "cdk deploy -q -c @context=dev --all",
    "deploy:test": "cdk deploy -q -c @context=test --all",
    "run:admin": "sam local start-api -t cdk.out/sam-ReserveRecApi-Local-AdminApiStack-AdminApi.yaml --warm-containers LAZY --skip-pull-image 2>&1 | tr '\r' '\n'",
    "run:public": "sam local start-api -t cdk.out/sam-ReserveRecApi-Local-PublicApiStack-PublicApi.yaml --env-vars env.json --warm-containers LAZY --skip-pull-image --port 3001 2>&1 | tr '\r' '\n'",
    "run:admin:full": "yarn synth:local && yarn build:admin && yarn run:admin",
    "run:public:full": "yarn synth:local && yarn build:public && yarn run:public",
    "booking-workflow:poller": "node ./src/scripts/tools/local-testing/booking-workflow-subscriber-poller.js"
  },
  "devDependencies": {
    "esbuild": "^0.25.11",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.930.0",
    "@aws-sdk/client-cognito-identity": "^3.913.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.914.0",
    "@aws-sdk/client-secrets-manager": "^3.910.0",
    "@aws-sdk/client-sns": "^3.930.0",
    "@aws-sdk/client-sqs": "^3.930.0",
    "@aws-sdk/client-ssm": "^3.910.0",
    "@aws-sdk/client-sts": "^3.913.0",
    "@aws-sdk/util-dynamodb": "^3.914.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@smithy/protocol-http": "^5.3.3",
    "@smithy/signature-v4": "^5.3.3",
    "aws-cdk": "^2.1030.0",
    "aws-cdk-lib": "^2.220.0",
    "@aws-sdk/client-cloudformation": "^3.828.0",
    "@aws-sdk/client-lambda": "^3.828.0",
    "@aws-sdk/client-s3": "^3.906.0",
    "@aws-sdk/client-ses": "^3.906.0",
    "@aws-sdk/credential-provider-node": "^3.743.0",
    "@aws-sdk/lib-dynamodb": "^3.879.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "constructs": "^10.0.0",
    "fs-extra": "^11.3.2",
    "https": "^1.0.0",
    "luxon": "^3.7.2",
    "node-fetch": "2",
    "crypto": "^1.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "node-rsa": "^1.0.2",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  }
}
