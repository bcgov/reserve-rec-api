{
  "name": "reserve-rec-api",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "echo \"The build step is not required when using JavaScript!\" && exit 0",
    "cdk": "cdk",
    "test": "jest",
    "synth:local": "cdk synth -c @context=local --all",
    "synth:dev": "cdk synth -c @context=dev --all",
    "deploy:dev": "cdk deploy -c @context=dev --all",
    "run-admin": "sam local start-api -t cdk.out/sam-AdminApiStack-Local-AdminApi.yaml --env-vars env.json --warm-containers LAZY --skip-pull-image 2>&1 | tr '\r' '\n'",
    "run-admin-full": "yarn synth:local && yarn run-admin"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.895.0",
    "@aws-sdk/client-ssm": "^3.891.0",
    "aws-cdk": "^2.1029.2",
    "aws-cdk-lib": "^2.215.0",
    "constructs": "^10.0.0",
    "winston": "^3.17.0"
  }
}
